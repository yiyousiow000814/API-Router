  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:root-exe": "tauri build && node scripts/copy-root-exe.mjs && node scripts/open-root-exe.mjs",
    "ui:tauri": "node scripts/tauri-ui-check.mjs",
    "ui:check": "node scripts/run-ui-tauri-if-windows.mjs",
    "backend:check": "cargo test --manifest-path src-tauri/Cargo.toml --locked",
    "check:all": "npm run test && npm run backend:check && npm run ui:check",
    "build:root-exe:checked": "node scripts/run-ui-tauri-if-windows.mjs && npm run build:root-exe",
    "test": "vitest run"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.6",
